{
  "name": "waicalm-backend",
  "version": "1.0.0",
  "description": "Backend para WaiCalm - App de bienestar mental",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --files src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test:db": "ts-node --files src/scripts/testConnection.ts",
    "init:db": "ts-node --files src/scripts/initDatabase.ts",
    "clean": "rm -rf dist"
  },
  "keywords": ["mental-health", "wellness", "mongodb", "express"],
  "author": "WaiCalm Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}